{
	"schemaVersion": 1,
	"id": "tele-mc",
	"version": "${version}",
	"name": "TeleMC",
	"description": "Simple mod to extract telemetry data from Minecraft",
	"authors": [
		"Wade Brainerd"
	],
	"contact": {
		"homepage": "https://github.com/wadetb/tele-mc",
		"sources": "https://github.com/wadetb/tele-mc"
	},
	"license": "CC0-1.0",
	"icon": "assets/modid/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.example.ExampleMod"
		],
		"client": [
			"com.example.ExampleModClient"
		]
	},
	"mixins": [
		"modid.mixins.json",
		{
			"config": "modid.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
